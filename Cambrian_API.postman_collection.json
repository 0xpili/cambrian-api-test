              "raw": "{{base_url}}/solana/tokens?limit=50&offset=0",
              "host": ["{{base_url}}"],
              "path": ["solana", "tokens"],
              "query": [
                {
                  "key": "limit",
                  "value": "50"
                },
                {
                  "key": "offset",
                  "value": "0"
                }
              ]
            }
          },
          "response": []
        },
        {
          "name": "SOL Token Details",
          "request": {
            "method": "GET",
            "header": [],
