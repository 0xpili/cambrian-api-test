{
  "info": {
    "name": "Cambrian API - Solana DeFi Analytics",
    "description": "Complete collection for testing Cambrian API endpoints. Covers token research, price data, portfolio tracking, and pool analysis.",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "auth": {
    "type": "apikey",
    "apikey": [
      {
        "key": "key",
        "value": "X-API-KEY",
        "type": "string"
      },
      {
        "key": "value",
        "value": "{{api_key}}",
        "type": "string"
      }
    ]
  },
  "event": [
    {
      "listen": "prerequest",
      "script": {
        "type": "text/javascript",
        "exec": [
          ""
        ]
      }
    }
  ],
  "variable": [
    {
      "key": "base_url",
      "value": "https://opabinia.cambrian.network/api/v1",
      "type": "string"
    },
    {
      "key": "api_key",
      "value": "your_api_key_here",
      "type": "string"
    },
    {
      "key": "sol_address",
      "value": "So11111111111111111111111111111111111111112",
      "type": "string"
    }
  ],
  "item": [
    {
      "name": "üèÅ Basic Connectivity",
      "item": [
        {
          "name": "Latest Block",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/solana/latest-block",
              "host": ["{{base_url}}"],
              "path": ["solana", "latest-block"]
            }
          },
          "response": []
        }
      ]
    },
    {
      "name": "üî• Token Discovery",
      "item": [
        {
          "name": "Trending Tokens by Volume",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/solana/trending-tokens?order_by=volume_usd_24h&limit=10",
              "host": ["{{base_url}}"],
              "path": ["solana", "trending-tokens"],
              "query": [
                {
                  "key": "order_by",
                  "value": "volume_usd_24h"
                },
                {
                  "key": "limit",
                  "value": "10"
                }
              ]
            }
          },
          "response": []
        },
        {
          "name": "Trending Tokens by Price Change",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/solana/trending-tokens?order_by=price_change_24h&limit=10",
              "host": ["{{base_url}}"],
              "path": ["solana", "trending-tokens"],
              "query": [
                {
                  "key": "order_by",
                  "value": "price_change_24h"
                },
                {
                  "key": "limit",
                  "value": "10"
                }
              ]
            }
          },
          "response": []
