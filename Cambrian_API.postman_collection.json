              "raw": "{{base_url}}/solana/token-details?token_address={{sol_address}}",
              "host": ["{{base_url}}"],
              "path": ["solana", "token-details"],
              "query": [
                {
                  "key": "token_address",
                  "value": "{{sol_address}}"
                }
              ]
            }
          },
          "response": []
